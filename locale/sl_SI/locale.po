msgstr "Določite barvo besedila z uporabo RGB vrednosti (0-255 za vsako komponento)"

# Eligibility settings
msgid "plugins.generic.reviewerCertificate.settings.eligibility"
msgstr "Merila Upravičenosti"

msgid "plugins.generic.reviewerCertificate.settings.minimumReviews"
msgstr "Najmanjše Število Dokončanih Recenzij"

